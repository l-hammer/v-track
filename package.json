{
  "name": "v-track",
  "version": "0.8.10",
  "description": "一个基于Vue指令的埋点插件",
  "author": "LHammer <lhammer@qq.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "build": "NODE_ENV=production npm run build:es && npm run build:browser",
    "build:browser": "rollup --config build/rollup.config.browser.js && npm run size",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:docs": "vue-cli-service build",
    "test": "npm run lint && npm run test:unit",
    "lint": "vue-cli-service lint",
    "prepublishOnly": "npm run test && npm run build",
    "size": "gzip-size dist/v-track.min.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "dist/v-track.min.js",
  "module": "dist/v-track.esm.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^5.2.0",
    "element-ui": "^2.9.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "gzip-size-cli": "^3.0.0",
    "highlight.js": "^10.1.2",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.4",
    "vue-template-compiler": "^2.5.21"
  },
  "keywords": [
    "vue",
    "javascript",
    "track",
    "directive",
    "plugin",
    "vue-components"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/l-hammer/v-track.git"
  }
}
